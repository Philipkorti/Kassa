<Window x:Class="Kassa.UserAdd"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Kassa"
        mc:Ignorable="d"
        Title="UserAdd" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        
        <StackPanel Grid.Row="1" Orientation="Horizontal">
            <TextBlock>User-ID: </TextBlock>
            <TextBox></TextBox>
            <TextBlock>Vorname: </TextBlock>
            <TextBox></TextBox>
            <TextBlock>Nachname: </TextBlock>
            <TextBox></TextBox>
            <TextBlock>Rechhte: </TextBlock>
            <ComboBox Name="rechteaswahl" Margin="0,60,0,50" SelectedValue="Kassa">
            </ComboBox>
            
        </StackPanel>
    </Grid>
</Window>
